mixin videoBlock(video ={})
    .videoBlock
        a(href=routes.videoDetail(video.id))
            video.videoBlock__thumbnail(controls=true)
                source(src="https://archive.org/download/BigBuckBunny_124/Content/big_buck_bunny_720p_surround.mp4")
            h4.videoBlock__title=video.title
            h6.videoBlock__views=video.views